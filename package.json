{
  "name": "BitBrothers",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --silent"
  },
  "jest": {
    "preset": "jest-expo",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/library/rave/**",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "coverageReporters": [
      "text"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|/.*|@unimodules/.*|unimodules|tcomb-form-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|sentry-expo|native-base)"
    ],
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "collectCoverage": true
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.8.0",
    "@types/react-native": "^0.62.2",
    "expo": "^36.0.0",
    "expo-analytics": "^1.0.15",
    "expo-av": "~8.0.0",
    "expo-font": "~8.0.0",
    "fetch-mock-jest": "^1.1.0-beta.3",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^3.0.1",
    "react": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.1.tar.gz",
    "react-native-animatable": "^1.3.3",
    "react-native-elements": "^1.2.7",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-modal": "^11.5.6",
    "react-native-paper": "^3.8.0",
    "react-native-responsive-screen": "^1.4.0",
    "react-native-screens": "2.0.0-alpha.12",
    "react-native-swiper": "^1.6.0",
    "react-native-table-component": "^1.2.1",
    "react-native-vector-icons": "^6.6.0",
    "react-native-web": "^0.11.7",
    "react-navigation": "4.0.10",
    "react-navigation-stack": "^1.10.3",
    "react-redux": "^7.1.3",
    "react-test-renderer": "^16.8.3",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-persist": "^5.10.0",
    "reflect-metadata": "^0.1.13",
    "styled-components": "^4.4.1",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "babel-preset-expo": "^8.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "jest-expo": "^36.0.0",
    "react-dom": "^16.9.0",
    "react-test-renderer": "^16.8.3",
    "redux-devtools": "^3.5.0",
    "redux-mock-store": "^1.5.4"
  },
  "private": true
}
